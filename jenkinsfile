pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh 'mvn compile'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Nexus'){
steps { 
 sh 'mvn deploy'
}
}
stage('Tomcat') {
            steps {
            sh "wget --user admin --password admin123 http://54.174.240.88:8081/nexus/service/local/repositories/releases/content/com/web/cal/WebAppCal/${DnewVersion}/WebAppCal-${DnewVersion}.war"
		sh "sudo cp WebAppCal-${DnewVersion}.war  /home/centos/apache-tomcat-7.0.94/webapps/"}
        }
}
}

